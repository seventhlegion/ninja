{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": ["plugin:react/recommended", "standard-with-typescript"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["react", "prettier"],
    "rules": {
        "semi": [2, "always"],
        "block-spacing": "warn",
        "space-before-blocks": "warn",
        "semi-spacing": "error",
        "semi-style": "error"
    }
}
